    private void btn_loginActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btn_loginActionPerformed
        // TODO add your handling code here:
        if (jTextField1.getText().isEmpty()) {
            JOptionPane.showMessageDialog(null,"Ingresa el Usuario", "error",JOptionPane.ERROR_MESSAGE);
        }
        else if (jPasswordField1.getText().isEmpty()) {
            JOptionPane.showMessageDialog(null,"Ingresa el Password", "error",JOptionPane.ERROR_MESSAGE);
        }
        else {
            if (JOptionPane.showConfirmDialog(null,"Desea ingresar al sistema?", "Confirmar",1)==0){

                String sql ="Select * from user where username=? and contrasena=? and funcion=?";
                try {
                    PreparedStatement ps = cn.prepareStatement(sql);
                    ps.setString(1, jTextField1.getText());
                    ps.setString(2, jPasswordField1.getText());
                    ps.setString(3, jcbx_cargo.getSelectedItem().toString());
                    ResultSet rs = ps.executeQuery();
                    if (rs.next()) {
                        jPanel2.setVisible(false);
                        habilitarmenu(jcbx_cargo.getSelectedItem().toString());
                        jlbl_conexion.setText("Conectado");
                    }
                    else {
                        JOptionPane.showMessageDialog(null, "Nombre de Usuario y Password Invalidas");
                    }
                }
                catch (Exception e) {

                }}}
    }//GEN-LAST:event_btn_loginActionPerformed

